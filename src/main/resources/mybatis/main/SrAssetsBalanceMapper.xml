<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baomidou.springmvc.mapper.main.SrAssetsBalanceMapper">
	<!--开启二级缓存-->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

	<resultMap id="BaseResultMap" type="com.baomidou.springmvc.model.main.SrAssetsBalance">
		<id column="id" property="id" />
		<result column="user_name" property="userName" />
		<result column="phone_number" property="phoneNumber" />
		<result column="investment_adviser" property="investmentAdviser" />
		<result column="register_date" property="registerDate" />
		<result column="regular_assets_total" property="regularAssetsTotal" />
		<result column="current_assets_total" property="currentAssetsTotal" />
		<result column="crp_assets_total" property="crpAssetsTotal" />
		<result column="aum_total" property="aumTotal" />
		<result column="aum_time_point" property="aumTimePoint" />
		<result column="mtime" property="mtime" />
		<result column="ctime" property="ctime" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, user_name AS userName, phone_number AS phoneNumber, investment_adviser AS investmentAdviser, register_date AS registerDate, regular_assets_total AS regularAssetsTotal, current_assets_total AS currentAssetsTotal, crp_assets_total AS crpAssetsTotal, aum_total AS aumTotal, aum_time_point AS aumTimePoint, mtime, ctime
    </sql>
</mapper>
