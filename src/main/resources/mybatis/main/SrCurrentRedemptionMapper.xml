<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baomidou.springmvc.mapper.main.SrCurrentRedemptionMapper">
	<!--开启二级缓存-->
	<!--<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

	<resultMap id="BaseResultMap" type="com.baomidou.springmvc.model.main.SrCurrentRedemption">
		<id column="id" property="id" />
		<result column="user_name" property="userName" />
		<result column="phone_number" property="phoneNumber" />
		<result column="product_name" property="productName" />
		<result column="redemption_date" property="redemptionDate" />
		<result column="redemption_amount" property="redemptionAmount" />
		<result column="investment_advisor" property="investmentAdvisor" />
		<result column="mtime" property="mtime" />
		<result column="ctime" property="ctime" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, user_name AS userName, phone_number AS phoneNumber, product_name AS productName, redemption_date AS redemptionDate, redemption_amount AS redemptionAmount, investment_advisor AS investmentAdvisor, mtime, ctime
    </sql>
</mapper>
