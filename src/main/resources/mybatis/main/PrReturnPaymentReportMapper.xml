<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baomidou.springmvc.mapper.main.PrReturnPaymentReportMapper">
	<!--开启二级缓存-->
	<!--<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

	<resultMap id="BaseResultMap" type="com.baomidou.springmvc.model.main.PrReturnPaymentReport">
		<id column="id" property="id" />
		<result column="phone_number" property="phoneNumber" />
		<result column="user_name" property="userName" />
		<result column="investment_advisor" property="investmentAdvisor" />
		<result column="user_identify" property="userIdentify" />
		<result column="product_name" property="productName" />
		<result column="amount" property="amount" />
		<result column="product_period" property="productPeriod" />
		<result column="product_interest_rate" property="productInterestRate" />
		<result column="interest_date" property="interestDate" />
		<result column="expiry_date" property="expiryDate" />
		<result column="mtime" property="mtime" />
		<result column="ctime" property="ctime" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, phone_number AS phoneNumber, user_name AS userName, investment_advisor AS investmentAdvisor, user_identify AS userIdentify, product_name AS productName, amount, product_period AS productPeriod, product_interest_rate AS productInterestRate, interest_date AS interestDate, expiry_date AS expiryDate, mtime, ctime
    </sql>
</mapper>
