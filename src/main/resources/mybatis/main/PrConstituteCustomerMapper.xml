<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baomidou.springmvc.mapper.main.PrConstituteCustomerMapper">
	<!--开启二级缓存-->
	<!--<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

	<resultMap id="BaseResultMap" type="com.baomidou.springmvc.model.main.PrConstituteCustomer">
		<id column="id" property="id" />
		<result column="investment_advisor" property="investmentAdvisor" />
		<result column="performance_customers_number" property="performanceCustomersNumber" />
		<result column="pure_new_vip_customers" property="pureNewVipCustomers" />
		<result column="reported_customers_number" property="reportedCustomersNumber" />
		<result column="allot_customers_number" property="allotCustomersNumber" />
		<result column="weekly_get_customers_number" property="weeklyGetCustomersNumber" />
		<result column="current_vip_customers_number" property="currentVipCustomersNumber" />
		<result column="mtime" property="mtime" />
		<result column="ctime" property="ctime" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, investment_advisor AS investmentAdvisor, performance_customers_number AS performanceCustomersNumber, pure_new_vip_customers AS pureNewVipCustomers, reported_customers_number AS reportedCustomersNumber, allot_customers_number AS allotCustomersNumber, weekly_get_customers_number AS weeklyGetCustomersNumber, current_vip_customers_number AS currentVipCustomersNumber, mtime, ctime
    </sql>
</mapper>
