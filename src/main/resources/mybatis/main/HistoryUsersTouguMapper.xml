<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baomidou.springmvc.mapper.main.HistoryUsersTouguMapper">
	<!--开启二级缓存-->
	<!--<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

	<resultMap id="BaseResultMap" type="com.baomidou.springmvc.model.main.HistoryUsersTougu">
		<id column="month" property="month" />
		<result column="mobile_no" property="mobileNo" />
		<result column="user_name" property="userName" />
		<result column="investment_adviser" property="investmentAdviser" />
		<result column="aum_total" property="aumTotal" />
		<result column="huoke_coefficient" property="huokeCoefficient" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        month, mobile_no AS mobileNo, user_name AS userName, investment_adviser AS investmentAdviser, aum_total AS aumTotal, huoke_coefficient AS huokeCoefficient
    </sql>
</mapper>
