<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baomidou.springmvc.mapper.main.KamAdvisorMapper">
	<!--开启二级缓存-->
	<!--<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

	<resultMap id="BaseResultMap" type="com.baomidou.springmvc.model.main.KamAdvisor">
		<id column="id" property="id" />
		<result column="tid" property="tid" />
		<result column="serial_number" property="serialNumber" />
		<result column="level" property="level" />
		<result column="system_name" property="systemName" />
		<result column="actual_name" property="actualName" />
		<result column="phone_number" property="phoneNumber" />
		<result column="remark" property="remark" />
		<result column="mtime" property="mtime" />
		<result column="ctime" property="ctime" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, tid, serial_number AS serialNumber, level, system_name AS systemName, actual_name AS actualName, phone_number AS phoneNumber, remark, mtime, ctime
    </sql>
</mapper>
