<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baomidou.springmvc.mapper.main.ClFormalUserVersionMapper">
	<!--开启二级缓存-->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

	<resultMap id="BaseResultMap" type="com.baomidou.springmvc.model.main.ClFormalUserVersion">
		<id column="id" property="id" />
		<result column="user_id" property="userId" />
		<result column="advisor_id" property="advisorId" />
		<result column="version_no" property="versionNo" />
		<result column="start_date" property="startDate" />
		<result column="end_date" property="endDate" />
		<result column="mtime" property="mtime" />
		<result column="ctime" property="ctime" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, user_id AS userId, advisor_id AS advisorId, version_no AS versionNo, start_date AS startDate, end_date AS endDate, mtime, ctime
    </sql>
</mapper>
